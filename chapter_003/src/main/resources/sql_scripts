create database wathesshop;
create table rolex(
    id serial primary key,
    name varchar(255),
    year_of_issue smallint,
    price integer
);
insert into rolex(name, year_of_issue, price) values ('Daytona', 2010, 4000);
select *from rolex;
update rolex set price = 4500;
select *from rolex;
delete from rolex ;
drop table rolex;